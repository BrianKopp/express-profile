{
  "name": "express-profile",
  "version": "0.0.1",
  "description": "A simple express middleware to expose nodejs profiling functions so you can profile your applications with zero downtime.",
  "main": "lib/index.js",
  "scripts": {
    "prebuild": "npm run lint",
    "build": "tsc",
    "lint": "tslint -c tslint.json -p tsconfig.json",
    "lint-fix": "tslint -c tslint.json -p tsconfig.json --fix",
    "prepare": "npm run build",
    "prepublishOnly": "npm run test && npm run lint",
    "test": "mocha './test/**/*.spec.js' --require ts-node/register",
    "deploy": "npm run build && npm publish"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/briankopp/express-profile.git"
  },
  "keywords": [
    "express",
    "inspect",
    "profiler",
    "profile"
  ],
  "author": "Brian Kopp",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/briankopp/express-profile/issues"
  },
  "homepage": "https://github.com/briankopp/express-profile#readme",
  "dependencies": {
    "winston": "^3.2.1"
  },
  "devDependencies": {
    "@types/express": "^4.17.6",
    "@types/node": "^14.0.13",
    "@types/winston": "^2.4.4",
    "chai": "^4.2.0",
    "mocha": "^8.0.1",
    "rewire": "^5.0.0",
    "sinon": "^9.0.2",
    "ts-lint": "^4.5.1",
    "ts-node": "^8.10.2",
    "tslint": "^5.20.0",
    "typescript": "^3.8.3"
  }
}
